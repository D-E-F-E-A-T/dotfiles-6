# general
DEFAULT_USER='dark'
POWERLEVEL9K_MODE='awesome-patched'
#POWERLEVEL9K_PROMPT_ON_NEWLINE=true
#POWERLEVEL9K_RPROMPT_ON_NEWLINE=true
POWERLEVEL9K_CONTEXT_TEMPLATE="%n@%m"
POWERLEVEL9K_SHORTEN_DIR_LENGTH=1
POWERLEVEL9K_SHORTEN_DELIMITER=""
POWERLEVEL9K_SHORTEN_STRATEGY="truncate_from_right"
# left prompt
POWERLEVEL9K_LEFT_PROMPT_ELEMENTS=(root_indicator context ssh dir)
# right prompt
POWERLEVEL9K_RIGHT_PROMPT_ELEMENTS=(status dir_writable vcs rust_version background_jobs)
# colors
POWERLEVEL9K_NODE_VERSION_FOREGROUND='green'
POWERLEVEL9K_NODE_VERSION_BACKGROUND='black'
POWERLEVEL9K_GO_FOREGROUND='blue'
POWERLEVEL9K_GO_BACKGROUND='black'
# plenv
prompt_perl() {
  if command -v plenv > /dev/null 2>&1; then
    $1_prompt_segment "$0" "$2" "black" "white" $(plenv version | sed -e 's/ (set.*$//') 'PERL_ICON'
  fi
}
# ruby
prompt_ruby() {
  if command -v rvm-prompt > /dev/null 2>&1; then
    $1_prompt_segment "$0" "$2" "black" "red" $(rvm-prompt i v g) 'RUBY_ICON'
  fi
}
# python
prompt_python() {
  if which pyenv &> /dev/null; then
    $1_prompt_segment "$0" "$2" "black" "yellow" $(pyenv version | sed -e 's/ (set.*$//' | tr '\n' ' ' | sed 's/.$//') 'PYTHON_ICON'
  fi
}
# go
prompt_go() {
  local go_version
  go_version=$(go version 2>/dev/null | sed -E "s/.*go([0-9.]*).*/\1/")
  if [[ -n "$go_version" ]]; then
    "$1_prompt_segment" "$0" "$2" "green" "255" "$go_version" 'GO_ICON'
  fi
}
